{
  "name": "graphql-api-starter",
  "version": "0.0.1",
  "description": "GraphQL API Starter",
  "main": "src/server.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/server.ts",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "compose": "docker-compose --env-file ./.env up -d"
  },
  "keywords": [],
  "author": "Involved Tech <developers@involved.com.br>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "fastify": "^4.13.0",
    "graphql": "^16.8.0",
    "mercurius": "^12.2.0",
    "pino": "^8.15.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.22.10",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.22.11",
    "@fastify/cors": "^8.3.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "pino-pretty": "^10.2.0",
    "prettier": "^2.8.4",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  }
}
